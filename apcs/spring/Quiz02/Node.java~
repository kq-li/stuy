public class Node {
  private Entry _entry;
  private Node _prev, _next;

  public Node(_entry, _prev, _next) {
    _entry = entry;
    _prev = prev;
    _next = next;
  }

  public Entry getEntry() {
    return _entry;
  }

  public Node getPrev() {
    return _prev;
  }

  public Node getNext() {
    return _next;
  }

  public Entry setEntry(Entry e) {
    Entry ret = _entry;
    _entry = e;
    return ret;
  }

  public Node setPrev(Node n) {
    Node ret = _prev;
    _prev = n;
    return ret;
  }

  public Node setNext(Node n) {
    Node ret = _next;
    _next = n;
    return ret;
  }

  public String toString() {
    return _entry.toString();
  }

  public static void main(String[] args) {

  }
}
